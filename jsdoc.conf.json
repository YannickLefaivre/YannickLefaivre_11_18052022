{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "includes": ["./src", "README.md"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "template": "./node_modules/minami",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
